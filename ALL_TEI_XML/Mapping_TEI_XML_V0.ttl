#Lexicon Mapping goes here 

<#LexiconTriplesMap>
a rr:TriplesMap;
rr:logicalTable [ rr:sqlQuery """

Select  'WBOLexiconExample' as lexicon, e.lang 
from entry e 
limit 1;
    """ ];	
	rr:subjectMap [
		rr:template "http://exploreat.adaptcentre.ie/{lexicon}}" ;
		rr:class lemon:Lexicon ;
		rr:graph <http://exploreat.adaptcentre.ie/WBOLexicon_graph> ;] ;	

	rr:predicateObjectMap [
		rr:predicate lemon:language ;
		rr:objectMap [ rr:column "lang" ] ;
		
		rr:graph <http://exploreat.adaptcentre.ie/WBOLexicon_graph> ;];
.

<#LexiconEntryTriplesMap>
a rr:TriplesMap;
rr:logicalTable [ rr:sqlQuery """

Select  'WBOLexiconExample' as lexicon, e.id, f.type, o.orth, o.type as orthType, e.lang 
from entry e 
inner join form f on e.entryid = f.entry_entryId 
inner join orth o on o.Form_FormId= f.formid;

    """ ];	
	rr:subjectMap [
		rr:template "http://exploreat.adaptcentre.ie/{lexicon}" ;
		rr:class lemon:lexicon ;
		rr:graph <http://exploreat.adaptcentre.ie/WBOLexicon_graph> ;] ;	
		
	rr:predicateObjectMap [
		rr:predicate lemon:entry;
		rr:objectMap [ 
			rr:template "http://exploreat.adaptcentre.ie/LexicalEntry/{orth}" ;
		rr:graph <http://exploreat.adaptcentre.ie/WBOLexicon_graph> ;] ;
		];
.

<#LexicalEntryTriplesMap>
a rr:TriplesMap;
rr:logicalTable [ rr:sqlQuery """

Select  e.id, f.type, o.orth, o.type as orthType, e.lang 
from entry e 
inner join form f on e.entryid = f.entry_entryId 
inner join orth o on o.Form_FormId= f.formid;
    """ ];	
	rr:subjectMap [
		rr:template "http://exploreat.adaptcentre.ie/LexicalEntry/{orth}" ;
		rr:class lemon:LexicalEntry ;
		rr:graph <http://exploreat.adaptcentre.ie/WBOLexicon_graph> ;] ;	

	rr:predicateObjectMap [
		rr:predicate lemon:form ;
		rr:objectMap [ rr:column "orth" ;rr:language "bar";] ;
		rr:graph <http://exploreat.adaptcentre.ie/WBOLexicon_graph> ;];
		
	rr:predicateObjectMap [
		rr:predicate lemon:form;
		rr:objectMap [ 
			rr:template "http://exploreat.adaptcentre.ie/Form/{orth}" ;
		rr:graph <http://exploreat.adaptcentre.ie/WBOLexicon_graph> ;] ;
		];
.

<#FormTriplesMap>
a rr:TriplesMap;
rr:logicalTable [ rr:sqlQuery """

Select  o.orth, o.type as orthType, e.lang 
from entry e 
inner join form f on e.entryid = f.entry_entryId 
inner join orth o on o.Form_FormId= f.formid;
    """ ];	
	rr:subjectMap [
		rr:template "http://exploreat.adaptcentre.ie/Form/{orth}" ;
		rr:class lemon:form ;
		rr:graph <http://exploreat.adaptcentre.ie/WBOLexicon_graph> ;] ;	

	rr:predicateObjectMap [
		rr:predicate lemon:writtenRep ;
		rr:objectMap [ rr:column "orth" ;rr:language "bar";] ;
		rr:graph <http://exploreat.adaptcentre.ie/WBOLexicon_graph> ;];
		
	
.

